{
  "reporter": "stylint-stylish",
  "blocks": "never",
  "brackets": "never",
  "colons": "never",
  "colors": "always",
  "commaSpace": "always",
  "commentSpace": "always",
  "cssLiteral": "never",
  "depthLimit": "always",
  "duplicates": true,
  "efficient": "always",
	"exclude": [],
	"extendPref": false,
  "globalDupe": false,
	"groupOutputByFile": true,
	"indentPref": false,
	"leadingZero": "never",
	"maxErrors": false,
	"maxWarnings": false,
  "mixed": false,
	"mixins": [],
	"namingConvention": true,
	"namingConventionStrict": true,
	"none": "never",
  "noImportant": true,
	"parenSpace": false,
	"placeholders": "always",
	"prefixVarsWithDollar": "always",
	"quotePref": false,
  "semicolons": "never",
  "sortOrder": "grouped",
	"stackedProperties": "never",
	"trailingWhitespace": "never",
	"universal": false,
	"valid": true,
	"zeroUnits": "never",
	"zIndexNormalize": false
}